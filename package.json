  },
  "build": {
    "appId": "com.qashto.automadicom",
    "copyright": "Copyright Â© 2018 Quinton Ashley",
    "electronVersion": "4.0.6",
    "electronDownload": {
      "version": "4.0.6"
    },
    "mac": {
      "category": "public.app-category.game",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
